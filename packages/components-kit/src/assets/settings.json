{
  "props": {
    "AppLoader#default": {
      "components": {
        "before-children": [
          {
            "component": "Notification"
          },
          {
            "component": "IconsProvider"
          }
        ]
      }
    },
    "RedirectContainer#default": {
      "to": "/ComponentForm"
    },
    "Layout#form": {
      "mode": "TwoColumns",
      "header": [{ "component": "HeaderBar" }],
      "one": [{ "component": "SidePanel" }],
      "content": [{ "component": "ComponentForm" }]
    },
    "HeaderBar#default": {
      "logo": {
        "icon": "talend-logo-square",
        "isFull": false
      },
      "brand": {
        "label": "Talend Component Kit"
      }
    },
    "SidePanel#default": {
      "actions": [{ "componentId": "menu-form" }]
    },
    "ActionButton#menu-form": {
      "icon": "talend-burger",
      "label": "ComponentForm",
      "onClickDispatch": {
        "type": "MENU_COMPONENT_FORM_CLICKED",
        "cmf": {
          "routerPush": "/ComponentForm"
        }
      }
    },
    "ComponentForm#default": {
      "definitionURL": "/api/v1/forms/add",
      "uiSpecPath": "ui",
      "triggerURL": "/api/v1/application/action"
    }
  },
  "routes": {
    "path": "/",
    "component": "AppLoader",
    "indexRoute": {
      "component": "Redirect"
    },
    "childRoutes": [
      {
        "path": "ComponentForm",
        "component": "Layout",
        "componentId": "form"
      }
    ]
  }
}
