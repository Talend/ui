{
  "extends": "airbnb",
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "indent": ["error", "tab", { "SwitchCase": 1 }],
    "no-mixed-operators": "off",
    "no-tabs": "off",
    "no-param-reassign": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
