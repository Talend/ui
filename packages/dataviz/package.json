{
  "name": "@talend/react-dataviz",
  "version": "7.0.1",
  "description": "Talend charts and visualization components",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib-esm/index.js",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "license": "Apache-2.0",
  "scripts": {
    "pre-release": "yarn build:umd:dev && yarn build:umd:prod",
    "build:umd:dev": "talend-scripts build --umd --dev",
    "build:umd:prod": "talend-scripts build --umd",
    "build:lib": "talend-scripts build",
    "build:lib:esm": "talend-scripts build --esm",
    "build-storybook": "talend-scripts build-storybook",
    "extract-i18n": "i18next-scanner --config i18next-scanner.config.js",
    "lint": "talend-scripts lint",
    "start": "talend-scripts start-storybook -p 6006",
    "test": "cross-env TZ=UTC talend-scripts test",
    "test:cy": "cypress run --component --spec **.cy.tsx",
    "test:watch": "cross-env TZ=UTC talend-scripts test --watch",
    "test:cov": "cross-env TZ=UTC talend-scripts test --coverage"
  },
  "keywords": [
    "react",
    "talend",
    "chart"
  ],
  "author": "Talend Frontend <frontend@talend.com>",
  "homepage": "https://github.com/Talend/ui/tree/master/packages/dataviz#readme",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "dependencies": {
    "@talend/assets-api": "^1.5.1",
    "@talend/react-components": "^17.3.3",
    "@talend/design-tokens": "^3.4.3",
    "classnames": "^2.5.1",
    "d3": "^7.9.0",
    "date-fns": "^3.6.0",
    "geojson": "^0.5.0",
    "lodash": "^4.17.21",
    "recharts": "^2.15.3",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@cypress/react": "^7.0.3",
    "@cypress/webpack-dev-server": "^3.11.0",
    "@talend/design-system": "^11.5.0",
    "@talend/eslint-config": "^13.2.1",
    "@talend/eslint-plugin": "^1.3.1",
    "@talend/icons": "^7.11.0",
    "@talend/locales-tui-components": "^16.0.1",
    "@talend/locales-tui-dataviz": "^3.0.1",
    "@talend/react-components": "^17.3.3",
    "@talend/scripts-core": "^16.5.1",
    "@talend/scripts-config-babel": "^13.5.0",
    "@talend/scripts-config-react-webpack": "^16.8.2",
    "@talend/scripts-config-storybook-lib": "^5.5.0",
    "@talend/scripts-config-typescript": "^11.3.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/classnames": "^2.3.4",
    "@types/d3": "^7.4.3",
    "@types/cypress": "^1.1.6",
    "@types/geojson": "^7946.0.16",
    "@types/lodash": "^4.17.16",
    "@types/node": "^6.14.13",
    "@types/react": "^18.3.20",
    "@types/topojson-client": "^3.1.5",
    "@types/topojson-specification": "^1.0.5",
    "cypress": "^13.17.0",
    "cross-env": "^7.0.3",
    "i18next": "^23.16.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.5.0"
  },
  "peerDependencies": {
    "i18next": "^23.16.8",
    "react": "^18.3.1",
    "react-i18next": "^13.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
