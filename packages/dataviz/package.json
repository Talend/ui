{
  "name": "@talend/react-dataviz",
  "version": "3.0.1",
  "description": "Talend charts and visualization components",
  "main": "lib/index.js",
  "mainSrc": "src/index.ts",
  "types": "./lib/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "pre-release": "yarn build:umd:dev && yarn build:umd:prod",
    "build:umd:dev": "talend-scripts build --umd --dev",
    "build:umd:prod": "talend-scripts build --umd",
    "build:lib": "talend-scripts build",
    "build-storybook": "talend-scripts build-storybook",
    "extract-i18n": "tsc --jsx preserve --outDir tmp && i18next-scanner && rm -rf tmp",
    "lint": "talend-scripts lint",
    "start": "talend-scripts start-storybook -p 6006",
    "test": "cross-env TZ=Europe/Paris talend-scripts test",
    "test:cy": "cypress run --component --spec **.cy.tsx",
    "test:watch": "cross-env TZ=Europe/Paris talend-scripts test --watch",
    "test:cov": "cross-env TZ=Europe/Paris talend-scripts test --coverage",
    "test:demo": "talend-scripts build-storybook --quiet"
  },
  "keywords": [
    "react",
    "talend",
    "chart"
  ],
  "author": "Talend Frontend <frontend@talend.com>",
  "homepage": "https://github.com/Talend/ui/tree/master/packages/dataviz#readme",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "dependencies": {
    "@talend/assets-api": "^1.2.2",
    "@talend/react-components": "^11.2.2",
    "@talend/design-tokens": "^2.9.0",
    "classnames": "^2.3.2",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "geojson": "^0.5.0",
    "lodash": "^4.17.21",
    "recharts": "^2.8.0",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@cypress/react": "^7.0.3",
    "@cypress/webpack-dev-server": "^3.5.3",
    "@talend/bootstrap-theme": "^8.1.1",
    "@talend/icons": "^6.59.0",
    "@talend/locales-tui-components": "^9.5.0",
    "@talend/locales-tui-dataviz": "^0.4.6",
    "@talend/react-components": "^11.2.2",
    "@talend/scripts-core": "^14.0.1",
    "@talend/scripts-config-react-webpack": "^15.4.1",
    "@testing-library/react": "^12.1.5",
    "@types/classnames": "^2.3.1",
    "@types/d3": "^7.4.0",
    "@types/enzyme": "^3.10.13",
    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.14.197",
    "@types/react": "^17.0.65",
    "@types/topojson-client": "^3.1.1",
    "@types/topojson-specification": "^1.0.2",
    "cross-env": "^7.0.3",
    "i18next": "^20.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6"
  },
  "peerDependencies": {
    "i18next": "^20.1.0",
    "react": ">= 16.14.0",
    "react-i18next": "^11.18.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
