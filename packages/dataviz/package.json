{
  "name": "@talend/react-dataviz",
  "version": "2.8.3",
  "description": "Talend charts and visualization components",
  "main": "lib/index.js",
  "mainSrc": "src/index.ts",
  "types": "./lib/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "pre-release": "yarn build:umd:dev && yarn build:umd:prod",
    "build:umd:dev": "talend-scripts build:lib:umd --dev",
    "build:umd:prod": "talend-scripts build:lib:umd",
    "build:lib": "talend-scripts build:ts:lib",
    "build-storybook": "talend-scripts build-storybook",
    "extract-i18n": "tsc --jsx preserve --outDir tmp && i18next-scanner && rm -rf tmp",
    "lint": "yarn lint:es && yarn lint:style",
    "lint:es": "talend-scripts lint:es",
    "lint:style": "talend-scripts lint:style",
    "start": "talend-scripts start-storybook -p 6006",
    "test": "cross-env TZ=Europe/Paris talend-scripts test",
    "test:watch": "cross-env TZ=Europe/Paris talend-scripts test --watch",
    "test:cov": "cross-env TZ=Europe/Paris talend-scripts test --coverage",
    "test:demo": "talend-scripts build-storybook --quiet"
  },
  "keywords": [
    "react",
    "talend",
    "chart"
  ],
  "author": "Talend Frontend <frontend@talend.com>",
  "homepage": "https://github.com/Talend/ui/tree/master/packages/dataviz#readme",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "dependencies": {
    "@talend/assets-api": "^1.2.1",
    "@talend/react-components": "^8.0.0",
    "@talend/design-tokens": "^2.7.0",
    "classnames": "^2.3.1",
    "d3": "^7.4.4",
    "date-fns": "^2.28.0",
    "geojson": "^0.5.0",
    "lodash": "^4.17.21",
    "recharts": "^2.1.13",
    "topojson-client": "^3.1.0"
  },
  "devDependencies": {
    "@talend/bootstrap-theme": "^6.39.1",
    "@talend/icons": "^6.50.0",
    "@talend/locales-tui-components": "^6.44.11",
    "@talend/locales-tui-dataviz": "^0.4.6",
    "@talend/react-components": "^8.0.0",
    "@talend/scripts-core": "^12.0.0",
    "@talend/scripts-preset-react-lib": "^14.0.0",
    "@testing-library/react": "^12.1.5",
    "@types/classnames": "^2.3.1",
    "@types/d3": "^7.4.0",
    "@types/enzyme": "^3.10.12",
    "@types/geojson": "^7946.0.10",
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.47",
    "@types/topojson-client": "^3.1.1",
    "@types/topojson-specification": "^1.0.2",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "i18next": "^20.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.1"
  },
  "peerDependencies": {
    "i18next": "^20.1.0",
    "react": ">= 16.14.0 < 18.0.0",
    "react-i18next": "^11.8.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
