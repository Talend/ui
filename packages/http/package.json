{
  "name": "@talend/http",
  "version": "2.0.0",
  "description": "HTTP helper",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui"
  },
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Talend <frontend@talend.com>",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "mainSrc": "src/index.ts",
  "scripts": {
    "build:lib": "talend-scripts build:ts:lib",
    "build:dev": "talend-scripts build:lib:umd --dev",
    "build:prod": "talend-scripts build:lib:umd --prod",
    "pre-release": "yarn build:dev && yarn build:prod",
    "start": "echo nothing to start",
    "test": "talend-scripts test",
    "lint:es": "talend-scripts lint:es --format json -o eslint-report.json"
  },
  "devDependencies": {
    "@talend/scripts-core": "^11.5.1",
    "@talend/scripts-preset-react-lib": "^9.11.2",
    "enzyme": "^3.11.0",
    "fetch-mock": "^9.11.0",
    "node-fetch": "^2.6.7",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "roots": [
      "src",
      "scripts",
      "__tests__"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/../../test-setup.js"
    ]
  },
  "files": [
    "/lib",
    "/src"
  ]
}
