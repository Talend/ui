{
  "name": "@talend/http",
  "version": "4.0.1",
  "description": "HTTP helper",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui"
  },
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Talend <frontend@talend.com>",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "mainSrc": "src/index.ts",
  "exports": {
    ".": {
      "import": "./lib-esm/index.js",
      "require": "./lib/index.js"
    },
    "./lib/*": {
      "import": "./lib-esm/*",
      "require": "./lib/*"
    }
  },
  "scripts": {
    "build:lib": "talend-scripts build",
    "build:lib:esm": "talend-scripts build --esm",
    "start": "echo nothing to start",
    "test": "talend-scripts test",
    "lint": "talend-scripts lint"
  },
  "devDependencies": {
    "@talend/eslint-config": "^14.0.0",
    "@talend/eslint-plugin": "^1.7.0",
    "@talend/scripts-core": "^17.0.0",
    "@talend/scripts-config-babel": "^13.9.0",
    "@talend/scripts-config-react-webpack": "^17.0.0",
    "@talend/scripts-config-typescript": "^12.0.0",
    "@types/jest": "^29.5.14",
    "@types/node-fetch": "^2.6.13",
    "node-fetch": "^2.7.0",
    "react-dom": "^18.3.1",
    "react": "^18.3.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "roots": [
      "src",
      "scripts",
      "__tests__"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/../../test-setup.js"
    ]
  },
  "files": [
    "/lib",
    "/lib-esm",
    "/src"
  ]
}
