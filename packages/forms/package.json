    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "bootstrap-sass": "3.3.7",
    "chokidar": "^1.6.1",
    "cpx": "1.5.0",
