    "react": "^16.8.6",
    "react-ace": "6.2.0",
    "react-dom": "^16.8.6",
    "react-i18next": "^7.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "roots": [
      "src",
      "__mocks__"
    ],
    "testRegex": "&*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../test-setup.js"
    ]
  },
  "publishConfig": {
    "access": "public"
