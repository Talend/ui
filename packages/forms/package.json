    "react-redux": "5.0.5",
    "redux": "3.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/../../node_modules/babel-jest"
    },
    "roots": [
      "src",
      "__mocks__"
    ],
    "testRegex": "&*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js"
  },
  "publishConfig": {
    "access": "public"
