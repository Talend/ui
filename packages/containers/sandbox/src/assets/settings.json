{
  "actions": {
    "menu:HeaderBar": {
      "id": "menu:HeaderBar",
      "label": "HeaderBar",
      "icon": "talend-arrow-right",
      "iconTransform": "rotate-270",
      "payload": {
        "type": "MENU_LINK",
        "cmf": {
          "routerReplace": "/HeaderBar"
        }
      }
    },
    "menu:ComponentForm": {
      "icon": "talend-burger",
      "label": "ComponentForm",
      "onClickDispatch": {
        "type": "MENU_COMPONENT_FORM_CLICKED",
        "cmf": {
          "routerPush": "/ComponentForm"
        }
      }
    }
  },
  "props": {
    "AppLoader#default": {
      "components": {
        "before-children": [
          {
            "component": "Notification"
          },
          {
            "component": "IconsProvider"
          }
        ]
      }
    },
    "Redirect#default": {
      "path": "/HeaderBar"
    },
    "HeaderBar#default": {
      "keepComponentState": true,
      "logo": {
        "id": "header-bar-demo-logo",
        "icon": "talend-world",
        "isFull": false
      },
      "brand": {
        "id": "header-bar-demo-brand",
        "label": "Choose a product"
      },
      "products": {
        "id": "header-bar-demo-brands",
        "items": [
          {
            "icon": "talend-star",
            "label": "Static menu entry",
            "dispatch": {
              "type": "CUSTOM_ACTION_TYPE",
              "payload": {
                "foo": "bar"
              }
            }
          }
        ]
      },
      "productsUrl": "/api/mock/header-bar/products-list"
    },
    "SidePanel#default": {
      "actionIds": [
        "menu:HeaderBar",
        "menu:ComponentForm"
      ]
    },
    "Layout#default": {
      "mode": "TwoColumns",
      "header": { "component": "HeaderBar" },
      "one": { "component": "SidePanel" },
      "content": {}
    },
    "Layout#HeaderBar": {
      "mode": "TwoColumns",
      "header": { "component": "HeaderBar" },
      "one": { "component": "SidePanel" },
      "content": {}
    },
    "ComponentForm#default": {
      "definitionURL": "/api/v1/forms/example",
      "uiSpecPath": "ui",
      "triggerURL": "/api/v1/application/action",
      "className": "full-form"
    }
  },
  "routes": {
    "path": "/",
    "component": "AppLoader",
    "indexRoute": {
      "component": "Redirect"
    },
    "childRoutes": [
      {
        "path": "HeaderBar",
        "component": "Layout",
        "componentId": "HeaderBar"
      },
      {
        "path": "ComponentForm",
        "component": "ComponentFormSandbox"
      },
      {
        "path": "*",
        "component": "Redirect"
      }
    ]
  }
}
