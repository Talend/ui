{
  "name": "react-talend-containers",
  "description": "Provide connected components aka containers for react-cmf based on react-talend-components.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel -d lib ./src/ && rimraf lib/**/*.test.js && cpx './src/**/*.scss' ./lib",
    "start": "start-storybook -p 6007",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint:es": "eslint --config .eslintrc src",
    "lint": "npm run lint:es",
    "watch": "npm-watch",
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook"
  },
  "watch": {
    "prepublish": "src/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "keywords": [
    "react",
    "bootstrap"
  ],
  "author": "Jean-Michel FRANCOIS <jmfrancois@talend.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Talend/react-talend-containers/issues"
  },
  "homepage": "https://github.com/Talend/react-talend-containers#readme",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@storybook/react": "3.1.9",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "bootstrap-sass": "3.3.7",
    "bootstrap-talend-theme": "^0.100.0",
    "bson-objectid": "1.1.5",
    "classnames": "2.2.5",
    "codacy-coverage": "^2.0.0",
    "cpx": "1.5.0",
    "css-loader": "0.28.2",
    "enzyme": "^2.7.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "immutable": "3.8.1",
    "invariant": "2.2.2",
    "jest": "20.0.3",
    "jest-cli": "20.0.3",
    "jsdoc": "^3.4.1",
    "lodash": "4.17.4",
    "node-sass": "4.5.3",
    "react": "15.5.4",
    "react-addons-css-transition-group": "15.5.2",
    "react-addons-test-utils": "15.5.1",
    "react-bootstrap": "0.31.0",
    "react-cmf": "^0.100.0",
    "react-css-transition": "^0.7.4",
    "react-dom": "15.5.4",
    "react-i18next": "^1.8.0",
    "react-redux": "5.0.5",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-storybook-cmf": "^0.1.3",
    "react-stub-context": "^0.7.0",
    "react-talend-components": "^0.100.0",
    "react-test-renderer": "15.5.4",
    "react-virtualized": "^9.1.0",
    "redux": "3.6.0",
    "redux-batched-actions": "0.2.0",
    "redux-thunk": "2.2.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "4.1.1",
    "style-loader": "^0.13.1",
    "talend-icons": "^0.100.0",
    "uuid": "3.0.1",
    "webpack": "^1.14.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "src/.*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/styleMock.js"
    }
  },
  "peerDependencies": {
    "immutable": "3.8.1",
    "invariant": "2.2.2",
    "lodash": "4.17.4",
    "react": "15.5.4",
    "react-bootstrap": "0.31.0",
    "react-cmf": "^0.100.0",
    "react-redux": "5.0.5",
    "react-talend-components": "^0.100.0",
    "reselect": "^2.5.4"
  },
  "version": "0.100.0"
}
