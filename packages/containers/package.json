    "react-dom": "^15.4.2",
    "react-i18next": "^1.8.0",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-storybook-cmf": "^0.1.3",
    "react-stub-context": "^0.7.0",
    "react-talend-components": "^0.70.0",
    "react-test-renderer": "^15.4.2",
    "redux": "3",
