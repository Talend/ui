{
	"routes": {
		"path": "/",
		"component": "AppLoader",
		"indexRoute": {
      "component": "HomeListView",
      "componentId": "homepage"
		},
		"childRoutes": [
			{
				"path": "403",
				"component": "HttpErrorView",
				"componentId": "403"
			},
			{
				"path": "404",
				"component": "HttpErrorView",
				"componentId": "404"
			},
			{
				"path": "500",
				"component": "HttpErrorView",
				"view": "fallback-http-error"
			},
			{
				"path": "internet-connection-problem",
				"component": "HttpErrorView",
				"view": "internet-connection-problem"
			}
		]
	}
}
