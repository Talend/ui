{
  "name": "@talend/log",
  "description": "Small wrapper over TraceKit, that allows to inject error reporting as redux-logger middleware",
  "main": "lib/errorTransformer.js",
  "license": "Apache-2.0",
  "scripts": {
    "prepublish": "babel -d lib ./src/",
    "start": "echo nothing to start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint:es": "eslint --config .eslintrc --ext .js src",
    "lint": "npm run lint:es"
  },
  "keywords": [
    "talend",
    "redux",
    "logger",
    "logging",
    "tracekit"
  ],
  "author": "Talend Frontend <frontend@talend.com>",
  "homepage": "https://github.com/Talend/ui/tree/master/packages/logging#readme",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "dependencies": {
    "tracekit": "^0.4.4",
    "whatwg-fetch": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.14.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "setupFiles": [
      "./env__tests.js"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "version": "0.81.0"
}
