{
  "name": "talend-log",
  "description": "Small wrapper over TraceKit, that allows to inject error reporting as redux-logger middleware",
  "main": "lib/errorTransformer.js",
  "scripts": {
    "prepublish": "babel -d lib ./src/",
    "start": "echo nothing to start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint:es": "eslint --config .eslintrc --ext .js src",
    "lint": "npm run lint:es"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "keywords": [
    "redux",
    "logger",
    "logging",
    "tracekit"
  ],
  "author": "Victor Musienko <vmusienko@talend.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "homepage": "https://github.com/Talend/ui/packages/logging#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.14.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "jest": "20.0.3",
    "jest-cli": "20.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "setupFiles": [
      "./env__tests.js"
    ]
  },
  "version": "0.95.0",
  "dependencies": {
    "tracekit": "^0.4.4",
    "whatwg-fetch": "^2.0.2"
  }
}
