{
  "name": "react-talend-components",
  "description": "Set of react widgets.",
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "author": "jsomsanith <jsomsanith@talend.com>",
  "bugs": {
    "url": "https://github.com/Talend/react-talend-components/issues"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:slimerjs": "slimerjs stories/slimer.js",
    "lint:style": "sass-lint -v -q",
    "lint:es": "eslint --config .eslintrc src",
    "lint": "npm run lint:es && npm run lint:style",
    "prepublish": "babel -d lib ./src/ && rimraf lib/**/*.test.js && cpx \"./src/**/*.scss\" lib",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "watch": "npm-watch",
    "cpx": "cpx './src/**/*.scss' ./lib"
  },
  "watch": {
    "prepublish": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": "js,scss"
    }
  },
  "keywords": [
    "react",
    "talend",
    "components",
    "bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-talend-components"
  },
  "dependencies": {
    "react-autowhatever": "^7.0.0",
    "react-debounce-input": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "cpx": "^1.5.0",
    "rimraf": "^2.5.4"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testPathDirs": [
      "src",
      "__mocks__"
    ],
    "testRegex": "&*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/styleMock.js"
    }
  },
  "peerDependencies": {
    "bootstrap-sass": "^3.3.7",
    "bootstrap-talend-theme": "^0.60.4",
    "classnames": "^2.2.5",
    "invariant": "^2.2.1",
    "react": "^15.4.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-bootstrap": "0",
    "talend-icons": "^0.60.4",
    "uuid": "^2.0.3"
  },
  "version": "0.60.4"
}
