{
  "name": "react-talend-components",
  "description": "Set of react widgets.",
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "author": "jsomsanith <jsomsanith@talend.com>",
  "bugs": {
    "url": "https://github.com/Talend/react-talend-components/issues"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:slimerjs": "slimerjs stories/slimer.js",
    "lint:style": "sass-lint -v -q",
    "lint:es": "eslint --config .eslintrc src",
    "lint": "npm run lint:es && npm run lint:style",
    "prepublish": "babel -d lib ./src/ && rimraf lib/**/*.test.js && cpx \"./src/**/*.scss\" lib",
    "start": "start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "watch": "npm-watch",
    "cpx": "cpx './src/**/*.scss' ./lib"
  },
  "watch": {
    "prepublish": {
      "patterns": [
        "src/**/*"
      ],
      "extensions": "js,scss"
    }
  },
  "keywords": [
    "react",
    "talend",
    "components",
    "bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-talend-components"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "react-autowhatever": "^7.0.0",
    "react-debounce-input": "^2.4.2",
    "react-virtualized": "^9.3.0"
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.3.0",
    "@kadira/storybook": "^2.35.0",
    "autoprefixer": "^6.7.2",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "bootstrap-sass": "3.3.7",
    "bootstrap-talend-theme": "^0.95.0",
    "classnames": "2.2.5",
    "codacy-coverage": "^2.0.0",
    "cpx": "1.5.0",
    "css-loader": "0.28.2",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "immutable": "3.8.1",
    "invariant": "2.2.2",
    "jest": "20.0.3",
    "keycode": "2.1.9",
    "node-sass": "4.5.3",
    "npm-watch": "^0.1.6",
    "postcss-loader": "^1.3.1",
    "react": "15.5.4",
    "react-a11y": "^0.3.3",
    "react-addons-css-transition-group": "15.5.2",
    "react-addons-test-utils": "15.5.1",
    "react-bootstrap": "0.31.0",
    "react-css-transition": "^0.7.4",
    "react-dom": "15.5.4",
    "react-storybook-addon-props-combinations": "^0.3.0",
    "react-test-renderer": "15.5.4",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "4.1.1",
    "slimerjs": "^0.10.2",
    "storyshots": "3.2.2",
    "style-loader": "^0.13.1",
    "talend-icons": "^0.95.0",
    "uuid": "3.0.1",
    "webpack": "^1.14.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "roots": [
      "src",
      "__mocks__"
    ],
    "testRegex": "&*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/styleMock.js"
    }
  },
  "peerDependencies": {
    "bootstrap-sass": "3.3.7",
    "bootstrap-talend-theme": "^0.95.0",
    "classnames": "2.2.5",
    "invariant": "2.2.2",
    "keycode": "2.1.9",
    "react": "15.5.4",
    "react-addons-css-transition-group": "15.5.2",
    "react-bootstrap": "0.31.0",
    "react-css-transition": "^0.7.4",
    "talend-icons": "^0.95.0",
    "uuid": "3.0.1"
  },
  "version": "0.95.0"
}
