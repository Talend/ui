  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "homepage": "https://github.com/Talend/ui",
  "devDependencies": {
    "autoprefixer": "^6.7.5",
    "babel-cli": "6.24.1",
    "babel-preset-es2015": "6.14.0",
    "css-loader": "0.28.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.9.0",
    "node-sass": "4.5.3",
    "path": "^0.12.7",
    "phantomcss": "^1.1.3",
    "postcss-loader": "^1.3.1",
    "sass-lint": "^1.9.1",
    "sass-loader": "6.0.5",
    "slimerjs": "^0.10.2",
