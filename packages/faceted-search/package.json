{
  "name": "@talend/react-faceted-search",
  "version": "0.2.2",
  "description": "Faceted search",
  "main": "lib/index.js",
  "mainSrc": "src/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "test": "cross-env TZ=Europe/Paris jest",
    "test:watch": "cross-env TZ=Europe/Paris jest --watch",
    "test:cov": "cross-env TZ=Europe/Paris jest --coverage",
    "test:demo": "build-storybook",
    "lint:style": "sass-lint -v -q",
    "lint:es": "eslint --config ../../.eslintrc src",
    "lint": "npm run lint:es && npm run lint:style",
    "prepublish": "babel -d lib ./src/ && rimraf lib/**/*.test.js && cpx -v \"./src/**/*.scss\" lib",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cpx": "cpx \"./src/**/*.scss\" ./lib"
  },
  "keywords": [
    "react",
    "talend",
    "faceted-search"
  ],
  "author": "Talend Frontend <frontend@talend.com>",
  "homepage": "https://github.com/Talend/ui/tree/master/packages/faceted-search#readme",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "dependencies": {
    "@talend/daikon-tql-client": "^1.2.0",
    "classnames": "^2.2.5",
    "invariant": "^2.2.2",
    "keycode": "^2.2.0",
    "lodash": "^4.17.4",
    "react-bootstrap": "0.32.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@talend/icons": "^4.10.0",
    "@talend/react-components": "^4.10.0",
    "@talend/react-forms": "^4.10.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme": "^3.9.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "eslint": "^4.0.0",
    "i18next": "^15.1.3",
    "jest": "^24.7.1",
    "jsdom": "^11.11.0",
    "node-sass": "^4.7.2",
    "prop-types": "^15.5.10",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react": "^16.8.6",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0"
  },
  "peerDependencies": {
    "@talend/icons": "^4.13.0",
    "@talend/react-cmf": "^4.13.0",
    "@talend/react-components": "^4.13.0",
    "@talend/react-forms": "^4.13.0",
    "i18next": "^15.1.3",
    "prop-types": "^15.5.10",
    "react-i18next": "^10.10.0",
    "react": "^16.8.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "roots": [
      "src",
      "__mocks__"
    ],
    "testRegex": "src/.*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "./test-setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "index.js"
    ],
    "snapshotSerializers": [
      "jest-serializer-html"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
