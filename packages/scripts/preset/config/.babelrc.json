{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            ">0.25%",
            "not op_mini all",
            "IE 11"
          ]
        }
      }
    ],
    "react"
  ],
  "ignore": [
    "**/**/*.css"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-export-extensions",
    "transform-object-assign",
    "transform-object-rest-spread"
  ]
}
