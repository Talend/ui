{
  "name": "@talend/react-faceted-search",
  "version": "1.0.2",
  "description": "Faceted search",
  "main": "lib/index.js",
  "mainSrc": "src/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "prepublishOnly": "talend-scripts build:lib:umd --dev && talend-scripts build:lib:umd --prod",
    "prepare": "talend-scripts build:lib",
    "test": "cross-env TZ=Europe/Paris talend-scripts test --silent",
    "test:watch": "cross-env TZ=Europe/Paris talend-scripts test --watch",
    "test:cov": "cross-env TZ=Europe/Paris talend-scripts test --coverage",
    "test:demo": "build-storybook",
    "lint:es": "talend-scripts lint:es",
    "lint:style": "sass-lint -v -q",
    "lint": "npm run lint:es && npm run lint:style",
    "start": "start-storybook -p 6006",
    "extract-i18n": "i18next-scanner --config i18next-scanner.config.js"
  },
  "keywords": [
    "react",
    "talend",
    "faceted-search"
  ],
  "author": "Talend Frontend <frontend@talend.com>",
  "homepage": "https://github.com/Talend/ui/tree/master/packages/faceted-search#readme",
  "bugs": {
    "url": "https://github.com/Talend/ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/ui.git"
  },
  "dependencies": {
    "@talend/daikon-tql-client": "^1.2.0",
    "classnames": "^2.2.5",
    "invariant": "^2.2.2",
    "keycode": "^2.2.0",
    "lodash": "^4.17.4",
    "react-bootstrap": "0.32.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@talend/bootstrap-theme": "^5.27.1",
    "@talend/icons": "^5.27.1",
    "@talend/locales-tui": "^5.24.0",
    "@talend/locales-tui-faceted-search": "0.9.0",
    "@talend/react-components": "^5.27.1",
    "@talend/scripts-core": "^6.8.2",
    "@talend/scripts-preset-react-lib": "^6.8.2",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^6.5.1",
    "husky": "^4.3.0",
    "i18next": "^15.1.3",
    "i18next-scanner": "^2.10.2",
    "lint-staged": "^10.4.2",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "sass-lint": "^1.13.1",
    "storybook-addon-i18next": "^1.2.1"
  },
  "peerDependencies": {
    "@talend/bootstrap-theme": ">= 5.27.1",
    "@talend/icons": ">= 5.27.1",
    "@talend/react-components": ">= 5.27.1",
    "i18next": "^15.1.3",
    "prop-types": "^15.5.10",
    "react": "^16.8.6",
    "react-i18next": "^10.10.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./{src,stories,test}/**/*.{ts,js,tsx}": "eslint --fix"
  }
}
