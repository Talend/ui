{
  "name": "react-flow-designer",
  "description": "Flow designer for react and redux",
  "version": "1.0.0",
  "main": "lib/index.js",
  "mainSrc": "src/index.js",
  "scripts": {
    "build": "talend-scripts build:lib",
    "clean": "npm run clean:lib && npm run clean:coverage",
    "clean:lib": "rimraf ./lib",
    "clean:coverage": "rimraf ./coverage",
    "flow": "flow",
    "lint": "npm run lint:es",
    "lint:es": "eslint --config .eslintrc src/**",
    "prepublish": "babel -d lib ./src/ && rimraf lib/**/*.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "talend-scripts test --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-flow-designer.git"
  },
  "author": "Axel Cateland <acateland@talend.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-flow": "^7.7.4",
    "@talend/scripts-core": "^3.1.0",
    "@talend/scripts-preset-react-lib": "^3.1.0",
    "coveralls": "3.0.9",
    "enzyme": "^3.1.0",
    "flow-bin": "^0.41.0",
    "flow-typed": "^2.0.0",
    "i18next": "^15.1.3",
    "immutable": "3.8.1",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-i18next": "^10.10.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.0.0",
    "redux": "3.6.0",
    "redux-mock-store": "1.2.1",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.4",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "semantic-release": "15.13.31",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.25"
  },
  "peerDependencies": {
    "immutable": "3",
    "react": "15 || ^16.0.0",
    "react-dom": "15 || ^16.0.0",
    "react-redux": "4",
    "redux": "3",
    "reselect": "2"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "d3-drag": "1.0.0",
    "d3-interpolate": "1.1.2",
    "d3-scale": "1.0.4",
