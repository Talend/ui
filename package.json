{
  "name": "react-talend-components",
  "description": "Set of react widgets.",
  "main": "lib/index.js",
  "license": "Apache-2.0",
  "author": "jsomsanith <jsomsanith@talend.com>",
  "bugs": {
    "url": "https://github.com/Talend/react-talend-components/issues"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint:style": "sass-lint -v -q",
    "lint:es": "eslint --config .eslintrc src",
    "lint": "npm run lint:es && npm run lint:style",
    "prepublish": "babel -d lib ./src/ && rm lib/**/*.test.js && cpx './src/**/*.scss' ./lib",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "react",
    "talend",
    "components",
    "bootstrap"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Talend/react-talend-components"
  },
  "dependencies": {},
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.2.2",
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "6.14.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "6.1.2",
    "babel-jest": "^15.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "classnames": "2.2.5",
    "codacy-coverage": "^2.0.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.0",
    "jest": "^16.0.1",
    "node-sass": "^3.10.1",
    "react": "^15.3.2",
    "react-a11y": "^0.3.3",
    "react-addons-test-utils": "^15.3.2",
    "react-bootstrap": "0",
    "react-dom": "^15.3.2",
    "react-test-renderer": "15",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.2",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.13.1",
    "talend-icons": "^0.0.0",
    "uuid": "^2.0.3",
    "webpack": "1.13.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testRegex": "src/.*\\.test\\.js$",
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "<rootDir>/test/styleMock.js"
    }
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.2",
    "react-bootstrap": "0",
    "uuid": "^2.0.3"
  }
}
