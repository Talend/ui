{
  "name": "react-flow-designer",
  "version": "0.0.7",
  "description": "Flow designer for react and redux",
  "main": "lib/react-flow-designer.js",
  "scripts": {
    "build": "npm run test  && npm run clean && webpack",
    "clean": "npm run clean:lib && npm run clean:coverage",
    "clean:lib": "rimraf ./lib",
    "clean:coverage": "rimraf ./coverage",
    "dev": "webpack --progress --colors --watch --env.dev",
    "lint": "npm run lint:es",
    "lint:es": "eslint --config .eslintrc src/app/**/*.js",
    "prepublish": "npm run build",
    "test": "jest --no-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:reset:snapshot": "jest --no-cache -u"
  },
  "author": "Axel Cateland <acateland@talend.com>",
  "license": "Apache-2.0",
  "jest": {
    "testRegex": "/.*\\.test(.js|.jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/test/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/fileMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-jest": "15.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-native-modules": "^6.9.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "immutable": "3.8.1",
    "jasmine-immutable-matchers": "1.0.1",
    "jest-cli": "15.1.1",
    "node-sass": "3.9.3",
    "react": "15.3.0",
    "react-addons-test-utils": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "react-test-renderer": "15.3.0",
    "redux": "3.6.0",
    "redux-mock-store": "1.1.2",
    "redux-thunk": "2.1.0",
    "reselect": "2.5.3",
    "sass-loader": "4.0.1",
    "style-loader": "0.13.1",
    "webpack": "2.1.0-beta.20"
  },
  "peerDependencies": {
    "immutable": "3.8.1",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "reselect": "2.5.3"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "d3-drag": "1.0.0",
    "d3-interpolate": "1.1.1",
    "d3-scale": "1.0.2",
    "d3-selection": "1.0.1",
    "d3-shape": "1.0.3",
    "d3-zoom": "1.0.2",
    "invariant": "2.2.1",
    "react-immutable-proptypes": "2.1.0"
  }
}
