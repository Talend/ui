{
  "name": "react-flow-designer",
  "version": "0.0.1",
  "description": "Flow designer for react and redux",
  "main": "lib/react-flow-designer.min.js",
  "scripts": {
    "build": "npm run clean && webpack --mode=build",
    "clean": "npm run clean:lib && npm run clean:coverage",
    "clean:lib": "rimraf ./lib",
    "clean:coverage": "rimraf ./coverage",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "testRegex": "/.*\\.test(.js|.jsx)$",
    "moduleNameMapper": {
      "^.+\\.(css|sass|scss)$": "<rootDir>/test/styleMock.js",
      "^.+\\.(gif|ttf|eot|svg)$": "<rootDir>/test/fileMock.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/redux-mock-store/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/immutable/",
      "<rootDir>/node_modules/jasmine-immutable-matchers",
      "<rootDir>/node_modules/react-redux/",
      "<rootDir>/node_modules/redux/",
      "<rootDir>/node_modules/enzyme/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-jest": "^14.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "immutable": "^3.8.1",
    "jasmine-immutable-matchers": "1.0.1",
    "jest-cli": "13.1.0",
    "node-sass": "^3.8.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-mock-store": "^1.1.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.1",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "3.5.2",
    "reselect": "^2.5.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "d3-drag": "^1.0.0",
    "d3-scale": "^1.0.2",
    "d3-shape": "^1.0.1",
    "d3-zoom": "^1.0.2"
  }
}
