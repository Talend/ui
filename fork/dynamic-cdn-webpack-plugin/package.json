  },
  "peerDependencies": {
    "webpack": "^5.102.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "all-contributors-cli": "^6.26.1",
    "codecov": "^3.8.3",
    "eslint": "^8.57.1",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^29.7.0",
    "mz": "^2.7.0",
    "rimraf": "^5.0.10",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-manifest-plugin": "^5.0.1"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.test.js",
      "<rootDir>/test/**/*.test.js"
    ]
